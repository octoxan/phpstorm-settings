<keymap version="1" name="Xan's Keymap" parent="VSCode OSX">
  <action id="ActivateTerminalToolWindow">
    <keyboard-shortcut first-keystroke="meta t" />
  </action>
  <action id="FileChooser.NewFile" />
  <action id="GotoSymbol">
    <keyboard-shortcut first-keystroke="shift meta o" />
  </action>
  <action id="NewFile" />
  <action id="SplitVertically">
    <keyboard-shortcut first-keystroke="meta back_slash" />
    <keyboard-shortcut first-keystroke="meta n" />
  </action>
  <action id="Terminal.SplitVertically">
    <keyboard-shortcut first-keystroke="meta n" />
  </action>
  <action id="ToggleZenMode">
    <keyboard-shortcut first-keystroke="meta k" second-keystroke="z" />
    <keyboard-shortcut first-keystroke="shift ctrl alt z" />
  </action>
</keymap>